{
  "name": "moedu-uniapp-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run dev:h5",
    "build": "npm run build:h5",
    "build:h5": "cross-env NODE_ENV=production UNI_PLATFORM=h5 vue-cli-service uni-build --mode production",
    "build:mp-weixin": "cross-env NODE_ENV=production UNI_PLATFORM=mp-weixin vue-cli-service uni-build --mode production",
    "dev:h5": "cross-env UNI_PLATFORM=h5 vue-cli-service uni-serve --mode local",
    "dev:mp-weixin": "cross-env UNI_PLATFORM=mp-weixin vue-cli-service uni-build --watch --mode local",
    "info": "node node_modules/@dcloudio/vue-cli-plugin-uni/commands/info.js",
    "lf": "find . -type f | grep -v \"node_modules\" | xargs dos2unix",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@dcloudio/uni-app-plus": "^2.0.0-32920210927002",
    "@dcloudio/uni-h5": "^2.0.0-32920210927002",
